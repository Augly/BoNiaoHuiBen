.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1150914_7muyg41yh3p.eot?t=1555637850499'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1150914_7muyg41yh3p.eot?t=1555637850499#iefix')
      format('embedded-opentype'),
    /* IE6-IE8 */
      url('data:application/x-font-woff2;charset=utf-8;base64,')
      format('woff2'),
    url('//at.alicdn.com/t/font_1150914_7muyg41yh3p.woff?t=1555637850499')
      format('woff'),
    url('//at.alicdn.com/t/font_1150914_7muyg41yh3p.ttf?t=1555637850499')
      format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
      url('//at.alicdn.com/t/font_1150914_7muyg41yh3p.svg?t=1555637850499#iconfont')
      format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

