    <view class="special_item" hover-class="none" hover-stop-propagation="false" wx:for="{{characters}}" wx:for-index="index" wx:for-item="item">
      <image class="special_item_logo br50" src="{{item.src}}" mode="scaleToFill" lazy-load="false" binderror="" bindload="" />
      <text class="special_item_title" selectable="false" space="false" decode="false">
        {{item.title}}
      </text>
    </view>
  </view>
</view>
