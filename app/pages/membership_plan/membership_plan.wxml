        <text class="year_mincolor" selectable="false" space="false" decode="false">每次可借</text>
      </view>
      <view class="tab_item" hover-class="none" hover-stop-propagation="false">
        <text class=" year_color" selectable="false" space="false" decode="false">不限次</text>
        <text class="year_mincolor" selectable="false" space="false" decode="false">借阅次数</text>
      </view>
      <view class="tab_item" hover-class="none" hover-stop-propagation="false">
